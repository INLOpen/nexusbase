{
  "name": "nexusbase-client",
  "version": "0.1.0",
  "description": "TypeScript client for NexusBase TSDB",
  "main": "dist/client.js",
  "types": "dist/client.d.ts",
  "scripts": {
    "build": "tsc",
    "build:proto": "node src/proto/generate.js --ts_out=grpc_js:./src/proto -I ../../ api/tsdb/tsdb.proto",
    "prepublishOnly": "npm run build:proto && npm run build",
    "test": "jest"
  },
  "author": "Gemini Code Assist",
  "license": "Apache-2.0",
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "google-protobuf": "^3.21.2",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^20.11.24",
    "grpc-tools": "1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "^30.0.5",
    "protoc-gen-ts": "^0.8.7",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3"
  }
}
